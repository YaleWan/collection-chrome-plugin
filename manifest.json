{
  "manifest_version": 1,
  "name": "收藏夹",
  "description": "收藏夹网址导航",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons":{
    "16": "icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "omnibox": { "keyword" : "che" },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ]
}